{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./globals.css": "./dist/globals.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "analyze": "node scripts/analyze-build.js",
    "build": "pnpm clean && concurrently \"pnpm:build:js\" \"pnpm:build:css\" --names \"js,css\" --prefix-colors \"bgBlue,bgGreen\"",
    "build:analyze": "pnpm run build && pnpm run analyze",
    "build:css": "tailwindcss -i ./src/globals.css -o ./dist/globals.css --minify",
    "build:js": "tsup",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf .turbo",
    "dev": "concurrently \"pnpm:dev:js\" \"pnpm:dev:css\" --names \"js,css\" --prefix-colors \"bgBlue,bgGreen\"",
    "dev:css": "tailwindcss -i ./src/globals.css -o ./dist/globals.css --watch",
    "dev:js": "tsup --watch",
    "generate:component": "turbo gen react-component",
    "lint": "eslint . --max-warnings 100",
    "watch": "pnpm dev"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tiptap/core": "^3.15.3",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/extension-text-align": "^3.15.3",
    "@tiptap/extension-text-style": "^3.15.3",
    "@tiptap/extension-typography": "^3.15.3",
    "@tiptap/extension-underline": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "animejs": "^4.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "catalog:",
    "next-themes": "catalog:",
    "react": "catalog:",
    "react-day-picker": "^9.13.0",
    "react-dom": "catalog:",
    "react-hook-form": "catalog:",
    "react-hotkeys-hook": "^5.2.3",
    "recharts": "^3.6.0",
    "sonner": "catalog:",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "catalog:",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "catalog:",
    "@turbo/gen": "^2.8.3",
    "@types/react": "catalog:",
    "concurrently": "catalog:",
    "esbuild": "^0.27.3",
    "esbuild-plugin-preserve-directives": "catalog:",
    "eslint": "catalog:",
    "postcss": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
