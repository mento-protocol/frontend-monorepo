{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./globals.css": "./dist/globals.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "analyze": "node scripts/analyze-build.js",
    "build": "pnpm clean && concurrently \"pnpm:build:js\" \"pnpm:build:css\" --names \"js,css\" --prefix-colors \"bgBlue,bgGreen\"",
    "build:analyze": "pnpm run build && pnpm run analyze",
    "build:css": "tailwindcss -i ./src/globals.css -o ./dist/globals.css --minify",
    "build:js": "tsup",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "concurrently \"pnpm:dev:js\" \"pnpm:dev:css\" --names \"js,css\" --prefix-colors \"bgBlue,bgGreen\"",
    "dev:css": "tailwindcss -i ./src/globals.css -o ./dist/globals.css --watch",
    "dev:js": "tsup --watch",
    "generate:component": "turbo gen react-component",
    "lint": "eslint . --max-warnings 100"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tiptap/core": "^2.22.3",
    "@tiptap/extension-link": "^2.22.3",
    "@tiptap/extension-placeholder": "^2.22.3",
    "@tiptap/extension-text-align": "^2.22.3",
    "@tiptap/extension-text-style": "^2.22.3",
    "@tiptap/extension-typography": "^2.22.3",
    "@tiptap/extension-underline": "^2.22.3",
    "@tiptap/pm": "^2.22.3",
    "@tiptap/react": "^2.22.3",
    "@tiptap/starter-kit": "^2.22.3",
    "animejs": "^4.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.488.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@turbo/gen": "^2.5.8",
    "@types/react": "^19.1.2",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.0",
    "esbuild-plugin-preserve-directives": "^0.0.11",
    "eslint": "^9.24.0",
    "postcss": "^8.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
